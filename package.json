{
  "name": "@walkthru-earth/react-native-duckdb",
  "version": "0.1.2",
  "description": "High-performance native DuckDB bindings for React Native using Nitro Modules",
  "main": "lib/index",
  "module": "lib/index",
  "types": "lib/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "!src/__tests__",
    "!src/__mocks__",
    "react-native.config.js",
    "lib",
    "!lib/__tests__",
    "!lib/__mocks__",
    "nitrogen",
    "android/build.gradle",
    "android/gradle.properties",
    "android/fix-prefab.gradle",
    "android/CMakeLists.txt",
    "android/src",
    "ios/**/*.h",
    "ios/**/*.m",
    "ios/**/*.mm",
    "ios/**/*.cpp",
    "ios/**/*.swift",
    "ios/DuckDB.xcframework",
    "android/src/main/jniLibs",
    "cpp/duckdb.h",
    "app.plugin.js",
    "nitro.json",
    "*.podspec",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "postinstall": "tsc || exit 0;",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf android/build node_modules/**/android/build lib",
    "lint": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "lint-ci": "eslint \"**/*.{js,ts,tsx}\" -f @jamesacarr/github-actions",
    "typescript": "tsc",
    "specs": "tsc --noEmit false && nitrogen --logLevel=\"debug\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepare": "husky"
  },
  "keywords": [
    "react-native",
    "nitro",
    "duckdb",
    "database",
    "sql",
    "olap",
    "analytics",
    "parquet",
    "jsi"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/walkthru-earth/react-native-duckdb.git"
  },
  "author": "Walkthru Earth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/walkthru-earth/react-native-duckdb/issues"
  },
  "homepage": "https://github.com/walkthru-earth/react-native-duckdb#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@react-native/eslint-config": "0.82.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.03",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "nitrogen": "*",
    "prettier": "^3.3.3",
    "react": "19.1.1",
    "react-native": "0.82.0",
    "react-native-nitro-modules": "*",
    "ts-jest": "^29.4.6",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-nitro-modules": "*"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "warn",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/",
    "coverage/",
    "*.config.js"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false,
    "semi": false
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
